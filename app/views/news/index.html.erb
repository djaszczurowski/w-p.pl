<h1>Listing news</h1>

<table>
  <tr>
    <th>User</th>
    <th>Posted</th>
    <th>Category</th>
    <th>Polish subject</th>
    <th>Polish news</th>
    <th>English subject</th>
    <th>English news</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @news.each do |news| %>
  <tr>
    <td><%= news.user_id %></td>
    <td><%= news.posted %></td>
    <td><%= news.category_id %></td>
    <td><%= news.polish_subject %></td>
    <td><%= news.polish_news %></td>
    <td><%= news.english_subject %></td>
    <td><%= news.english_news %></td>
    <td><%= link_to 'Show', news %></td>
    <td><%= link_to 'Edit', edit_news_path(news) %></td>
    <td><%= link_to 'Destroy', news, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New News', new_news_path %>
