<h1>Listing news</h1>

<table>
  <tr>
    <th>Posted</th>
    <th>Polishsubject</th>
    <th>Polishnews</th>
    <th>Englishsubject</th>
    <th>Englishnews</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @news.each do |news| %>
  <tr>
    <td><%= news.posted %></td>
    <td><%= news.polishSubject %></td>
    <td><%= news.polishNews %></td>
    <td><%= news.englishSubject %></td>
    <td><%= news.englishNews %></td>
    <td><%= link_to 'Show', news %></td>
    <td><%= link_to 'Edit', edit_news_path(news) %></td>
    <td><%= link_to 'Destroy', news, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New News', new_news_path %>
